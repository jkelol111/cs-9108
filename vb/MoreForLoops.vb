Module MoreForLoops
	Sub Main()
		For i As Integer = 1 To 50
			Console.WriteLine(i)
		Next
		Console.ReadLine()
		
		For myNumber As Integer = 1 To 10
			Console.WriteLine(myNumber)
		Next
		Console.ReadLine()
		
		For myNumber As Integer = 10 To 1 Step -1
			Console.WriteLine(myNumber)
		Next
		Console.ReadLine()
	End Sub
End Module
